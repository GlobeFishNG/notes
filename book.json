{
  "plugins": [
    "plantuml",
    "edit-link",
    "page-toc",
    "codeblock-filename",
    "todo"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/ufonion/notes/edit/master/",
      "label": "Edit Me"
    },
    "page-toc": {
      "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
      "position": "top"
    }
  }
}
